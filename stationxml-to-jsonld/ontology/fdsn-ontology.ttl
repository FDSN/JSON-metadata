#    Classes
#################################################################

###  file:/home/stefano/gitwork/stationxml-to-jsonld/ontology/fdsn-ontology.ttl#operationalPeriod
<file:/home/stefano/gitwork/stationxml-to-jsonld/ontology/fdsn-ontology.ttl#operationalPeriod> rdf:type owl:Class .


###  file:/home/stefano/gitwork/stationxml-to-jsonld/ontology/fdsn-ontology.ttl#@type
<file:/home/stefano/gitwork/stationxml-to-jsonld/ontology/fdsn-ontology.ttl#@type> rdf:type owl:Class ;
                                                                                   rdfs:subClassOf <file:/home/stefano/gitwork/stationxml-to-jsonld/ontology/fdsn-ontology.ttl#operationalPeriod> .


###  http://org.semanticweb.owlapi/error#Error1
<http://org.semanticweb.owlapi/error#Error1> rdf:type owl:Class .


###  http://org.semanticweb.owlapi/error#Error3
<http://org.semanticweb.owlapi/error#Error3> rdf:type owl:Class .


###  http://www.w3.org/2000/01/rdf-schema#Resource
rdfs:Resource rdf:type owl:Class .


###  http://www.w3.org/2004/02/skos/core#Concept
skos:Concept rdf:type owl:Class .


###  http://www.w3.org/2006/time#Instant
time:Instant rdf:type owl:Class .


###  http://www.w3.org/2006/time#ProperInterval
time:ProperInterval rdf:type owl:Class .


###  http://www.w3.org/2006/time#hasBeginning
time:hasBeginning rdf:type owl:Class ;
                  rdfs:subClassOf <file:/home/stefano/gitwork/stationxml-to-jsonld/ontology/fdsn-ontology.ttl#operationalPeriod> .


###  http://www.w3.org/ns/dcat#DataService
dcat:DataService rdf:type owl:Class .


###  http://www.w3.org/ns/org#Organization
org:Organization rdf:type owl:Class .


###  https://webservices.example.org/fdsn/terms#AccessStatus
fdsn:AccessStatus rdf:type owl:Class ;
                  rdfs:label """Access Status"""@en .


###  https://webservices.example.org/fdsn/terms#Comment
fdsn:Comment rdf:type owl:Class ;
             rdfs:comment """A free-text comment with optional subject, identifier, and validity period."""@en ;
             rdfs:label """Comment"""@en .


###  https://webservices.example.org/fdsn/terms#DataCenter
fdsn:DataCenter rdf:type owl:Class ;
                rdfs:subClassOf org:Organization ;
                dcterms:creator """""" ;
                rdfs:comment """An organization that (re)distributes seismic data and metadata, possibly serving multiple networks."""@en ;
                rdfs:label """Data Center"""@en .


###  https://webservices.example.org/fdsn/terms#Network
fdsn:Network rdf:type owl:Class ;
             rdfs:comment """A seismic network described by FDSN Station metadata."""@en ;
             rdfs:label """Network"""@en .


###  https://webservices.example.org/fdsn/terms#SeismicDataProvider
fdsn:SeismicDataProvider rdf:type owl:Class ;
                         rdfs:subClassOf org:Organization ;
                         rdfs:comment """An organization that operates seismic networks and provides data."""@en ;
                         rdfs:label """Seismic Data Provider"""@en .


###  https://webservices.example.org/fdsn/terms#Station
fdsn:Station rdf:type owl:Class ;
             rdfs:comment """""""A seismic station that belongs to a Network ;
  rdfs:subClassOf wgs:SpatialThing ."""""""@en ;
             rdfs:label """Station"""@en .


#################################################################
#    Individuals
#################################################################

###  http://www.co-ode.org/ontologies/ont.owl#IV
<http://www.co-ode.org/ontologies/ont.owl#IV> rdf:type owl:NamedIndividual ,
                                                       fdsn:Network ;
                                              fdsn:hasAccessStatus fdsn:open ;
                                              fdsn:operationalPeriod [ rdf:type time:ProperInterval ;
                                                                       time:hasBeginning [ rdf:type time:Instant ;
                                                                                           time:inXSDDateTime """1988-01-01T00:00:00Z"""^^xsd:dateTime
                                                                                         ]
                                                                     ] ;
                                              dcterms:description """INGV Italian National Seismic Network"""@en ;
                                              dcterms:identifier """https://doi.org/10.13127/SD/X0FXNH7QFY""" ;
                                              fdsn:sourceId """https://webservices.example.org/id/FDSN:IV"""^^xsd:anyURI ;
                                              rdfs:label """IV"""@en .


###  https://creativecommons.org/licenses/by/4.0/
<https://creativecommons.org/licenses/by/4.0/> rdf:type owl:NamedIndividual .


###  https://webservices.example.org/fdsn/terms#
<https://webservices.example.org/fdsn/terms#> rdf:type owl:NamedIndividual ;
                                              dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
                                              dcterms:title """FDSN core ontology (strict skeleton from JSON-LD context)""" .


###  https://webservices.example.org/fdsn/terms#Closed
fdsn:Closed rdf:type owl:NamedIndividual ,
                     skos:Concept ,
                     fdsn:AccessStatus ;
            skos:prefLabel """Closed"""@en .


###  https://webservices.example.org/fdsn/terms#INGV
fdsn:INGV rdf:type owl:NamedIndividual ,
                   org:Organization ,
                   fdsn:DataCenter ,
                   fdsn:SeismicDataProvider ;
          fdsn:operatesNetwork <http://www.co-ode.org/ontologies/ont.owl#IV> ;
          <https://www.w3.org/ns/org#siteAddress> """Via di Vigna Murata 605, Roma IT"""@it ;
          <https://www.w3.org/ns/org#Site> """Sede centrale""" .


###  https://webservices.example.org/fdsn/terms#Open
fdsn:Open rdf:type owl:NamedIndividual ,
                   skos:Concept ,
                   fdsn:AccessStatus ;
          skos:prefLabel """Open"""@en .


###  https://webservices.example.org/fdsn/terms#Partial
fdsn:Partial rdf:type owl:NamedIndividual ,
                      skos:Concept ,
                      fdsn:AccessStatus ;
             skos:prefLabel """Partial"@en .


###  https://webservices.example.org/fdsn/terms#open
fdsn:open rdf:type owl:NamedIndividual .


###  https://webservices.example.org/id/station/IV:ACER#ACER
<https://webservices.example.org/id/station/IV:ACER#ACER> rdf:type owl:NamedIndividual ,
                                                                   fdsn:Station ;
                                                          fdsn:hasAccessStatus fdsn:Open ;
                                                          fdsn:memberOfNetwork <http://www.co-ode.org/ontologies/ont.owl#IV> .


